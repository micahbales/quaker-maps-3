{
  "name": "quaker-maps-api",
  "author": "Micah Bales",
  "license": "+CAL LICENSE - SEE LICENSE FILE",
  "scripts": {
    "start": "nodemon index.js",
    "server": "concurrently --kill-others-on-fail \"npm run typescript\" \"npm start\"",
    "typescript": "tsc -w",
    "lint": "tslint './*.ts' --fix",
    "seed": "npm run seed:dev && npm run seed:test",
    "seed:dev": "PGDATABASE='quaker_maps_dev' node ./migrations/data/insertTestData.js",
    "seed:test": "PGDATABASE='quaker_maps_test' node ./migrations/data/insertTestData.js",
    "tests": "db-migrate down -e test && db-migrate up -e test && npm run seed:test && PGDATBASE=quaker_maps_test mocha tests/**/*.js"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "db-migrate": "^0.11.1",
    "db-migrate-pg": "^0.4.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.0.8",
    "@types/pg": "^7.4.10",
    "@types/supertest": "^2.0.6",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.4",
    "supertest": "^3.3.0",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  }
}
